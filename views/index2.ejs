<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="profile">
    <title>DOGGO Website</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <main>
        <!-- Navigation bar -->
        <header>
            <nav>
                <img class="logo" src="images/DOGGO.svg" alt="logo">
                <div class="links">
                    <a href="#">About</a>
                    <a href="#">Docs</a>
                    <button class="button_med" href="#">Login</button>
                </div>
            </nav>
        </header>
        <!-- Contents -->
        <section class="info">
            <h1 class="content_font1"><%=contentfont1 %></h1>
            <h4 class="content_font2"><%=contentfont2 %></h4>
            <div class="icons">

            <% obj_icon.forEach(function(icon){%>
                <div class="icon1">
                    <img src=<%= icon.icon %> >
                    <p class="icon_info"><%= icon.info%></p>
                </div>
                <div class="icon2">
                    <img src=<%= icon.icon2 %>>
                </div>
                <% }) %>

            </div>
        </section>
        <!-- Feature bar -->
         <section class="features">
             <!--Add Parameter that forEach loop-->
             <% obj_feature.forEach(function(feature){ %>
                
             <div class="feature">
                 <img src= <%= feature.img %>>
                 <div class="feature_info">
                     <p class="feature_top"><%= feature.top%></p>
                 </div>
             </div>
            <% }) %>
            <!-- Comment not use
             <div class="feature">
                <img src="images/talk logo.svg" alt="talk_logo">
                <div class="feature_info">
                    <p class="feature_more">Talk</p>
                </div>
            </div>
            -->
         </section>
    </main>
</body>
</html>